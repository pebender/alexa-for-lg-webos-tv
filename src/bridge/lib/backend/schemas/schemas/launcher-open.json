{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "title": "LG WebOS TV 'ssap://system.launcher/open' Response",
  "type": "object",
  "required": ["returnValue", "id", "sessionId"],
  "properties": {
    "returnValue": {
      "$id": "#root/returnValue",
      "title": "Returnvalue",
      "type": "boolean",
      "default": true
    },
    "id": {
      "$id": "#root/id",
      "title": "Id",
      "type": "string",
      "default": "",
      "pattern": "^.*$"
    },
    "sessionId": {
      "$id": "#root/sessionId",
      "title": "Sessionid",
      "type": "string",
      "default": "",
      "pattern": "^.*$"
    }
  }
}
